            <max_speed>10</max_speed>
            <left_steering_pid_gain>1000 10 100</left_steering_pid_gain>
            <left_steering_i_range>0 1</left_steering_i_range>
            <right_steering_pid_gain>1000 10 100</right_steering_pid_gain>
